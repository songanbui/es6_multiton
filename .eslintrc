{
  "extends": "airbnb",
  "ecmaFeatures": {
    "generators": true
  },
  "rules":{
    "strict":0,
    "func-names": 0,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0
  },
  "env": {
    "mocha":true,
    "node":true,
    "es6":true
  }
}